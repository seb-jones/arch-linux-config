urxvt.background:  #000000
urxvt.foreground:  #ffffff

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
Xft.dpi: 72

! Fonts
urxvt*font: xft:Hack-15
urxvt*boldFont: xft:Hack:bold:autohint=true
urxvt*italicFont: xft:Hack:italic:autohint=true
urxvt*boldItalicFont: xft:Hack:italic:bold:autohint=true

! Misc
urxvt*intensityStyles:        true
urxvt*skipBuiltinGlyphs:    true
urxvt*linespace:            0
urxvt.scrollBar: false
urxvt.saveLines: 10000
urxvt.cursorBlink: true
urxvt.pointerBlank: true
